{
  "name": "koa-server",
  "version": "0.0.1",
  "description": "koa server",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist && mkdir dist",
    "lint": "eslint index.js ./src/** ./test/**",
    "prestart": "npm run build",
    "start": "npm run watch",
    "watch": "nodemon --exec ts-node -- ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build.doc": "graphdoc -e http://localhost:4001/graphql -o ./doc/schema"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.5.0",
    "graphql": "^0.11.2",
    "graphql-relay": "^0.5.2",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-graphql": "^0.7.3",
    "koa-logger": "^3.0.1",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "koa-views": "^6.0.2",
    "mongoose": "^4.11.9",
    "mongoose-fill": "^1.6.0",
    "require-dir": "^0.3.2",
    "validator": "^8.1.0"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
